        "FractalDimension": null,
        "TheoreticalLogLogSlope": -2.5
      }
    ]
  },
  "scales": {
    "Description": "Generic log–log points for each system at multiple iterations / scales.",
    "schema": [
      {
        "name": "SystemID",
        "datatype": "string",
        "type": "raw",
        "Description": "FK to systems.SystemID."
      },
      {
        "name": "Iteration",
        "datatype": "integer",
        "type": "raw",
        "Description": "Discrete zoom level or rank index."
      },
      {
        "name": "BaseScale",
        "datatype": "number",
        "type": "lookup",
        "formula": "=_xlfn.INDEX(systems!BaseScale, _xlfn.MATCH({{SystemID}}, systems!SystemID, 0))",
        "Description": "Base scale pulled from systems."
      },
      {
        "name": "ScaleFactor",
        "datatype": "number",
        "type": "lookup",
        "formula": "=_xlfn.INDEX(systems!ScaleFactor, _xlfn.MATCH({{SystemID}}, systems!SystemID, 0))",
        "Description": "Scale factor pulled from systems."
      },
      {
        "name": "Scale",
        "datatype": "number",
        "type": "calculated",
        "formula": "={{BaseScale}}*({{ScaleFactor}}^{{Iteration}})",
        "Description": "X-axis variable (e.g., length scale, rank, degree)."
      },
      {
        "name": "Measure",
        "datatype": "number",
        "type": "raw",
        "Description": "Y-axis measured quantity at this scale."
      },
      {
        "name": "LogScale",
        "datatype": "number",
        "type": "calculated",
        "formula": "=_xlfn.LOG10({{Scale}})",
        "Description": "log10 of Scale."
      },
      {
        "name": "LogMeasure",
        "datatype": "number",
        "type": "calculated",
        "formula": "=_xlfn.LOG10({{Measure}})",
        "Description": "log10 of Measure (where positive)."
      }
    ],
