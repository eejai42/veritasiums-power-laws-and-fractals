  "system_stats": {
    "Description": "Statistical analysis of each system's log-log behavior, with rollups from scales and lookups to systems.",
    "primary_key": ["SystemStatsID"],
    "schema": [
      {
        "name": "SystemStatsID",
        "datatype": "string",
        "type": "raw",
        "is_primary_key": true,
        "Description": "Primary key for this stats record."
      },
      {
        "name": "System",
        "datatype": "string",
        "type": "relationship",
        "RelatedTo": "systems",
        "Description": "FK to systems table."
      },
      {
        "name": "AnalysisName",
        "datatype": "string",
        "type": "raw",
        "Description": "Human-readable name for this analysis run."
      },
      {
        "name": "Status",
        "datatype": "string",
        "type": "raw",
        "Description": "Status of analysis: draft, validated, published."
      },
      {
        "name": "SystemDisplayName",
        "datatype": "string",
        "type": "lookup",
        "formula": "=INDEX(systems!{{DisplayName}}, MATCH(system_stats!{{System}}, systems!{{SystemID}}, 0))",
        "Description": "Display name looked up from parent system."
      },
      {
        "name": "TheoreticalLogLogSlope",
        "datatype": "number",
        "type": "lookup",
        "formula": "=INDEX(systems!{{TheoreticalLogLogSlope}}, MATCH(system_stats!{{System}}, systems!{{SystemID}}, 0))",
        "Description": "Theoretical slope looked up from parent system."
      },
      {
        "name": "PointCount",
        "datatype": "integer",
        "type": "aggregation",
        "formula": "=COUNTIF(scales!{{System}}, {{System}})",
        "Description": "Rollup: Count of scale measurements for this system."
      },
      {
        "name": "MinLogScale",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=MINIFS(scales!{{LogScale}}, scales!{{System}}, {{System}})",
        "Description": "Rollup: Minimum log10(Scale) across child scales."
      },
      {
        "name": "MaxLogScale",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=MAXIFS(scales!{{LogScale}}, scales!{{System}}, {{System}})",
        "Description": "Rollup: Maximum log10(Scale) across child scales."
      },
      {
        "name": "MinLogMeasure",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=MINIFS(scales!{{LogMeasure}}, scales!{{System}}, {{System}})",
        "Description": "Rollup: Minimum log10(Measure) across child scales."
      },
      {
        "name": "MaxLogMeasure",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=MAXIFS(scales!{{LogMeasure}}, scales!{{System}}, {{System}})",
        "Description": "Rollup: Maximum log10(Measure) across child scales."
      },
      {
        "name": "DeltaLogMeasure",
        "datatype": "number",
        "type": "calculated",
        "formula": "={{MinLogMeasure}} - {{MaxLogMeasure}}",
        "Description": "Calculated: Difference between min and max log measure (numerator for slope)."
      },
      {
        "name": "DeltaLogScale",
        "datatype": "number",
        "type": "calculated",
        "formula": "={{MaxLogScale}} - {{MinLogScale}}",
        "Description": "Calculated: Difference between max and min log scale (denominator for slope)."
      },
      {
        "name": "EmpiricalLogLogSlope",
        "datatype": "number",
        "type": "calculated",
        "formula": "={{DeltaLogMeasure}} / {{DeltaLogScale}}",
        "Description": "Calculated: Slope of log-log line from empirical data."
      },
      {
        "name": "SlopeError",
        "datatype": "number",
        "type": "calculated",
        "formula": "={{EmpiricalLogLogSlope}} - {{TheoreticalLogLogSlope}}",
        "Description": "Calculated: Difference between empirical and theoretical slopes."
      }
    ],
