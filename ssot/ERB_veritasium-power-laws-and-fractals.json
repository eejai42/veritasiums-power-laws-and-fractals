  "system_stats": {
    "Description": "Aggregated log–log slope estimates per system, keeping slope separate from fractal dimension.",
    "schema": [
      {
        "name": "SystemID",
        "datatype": "string",
        "type": "raw",
        "Description": "FK to systems.SystemID."
      },
      {
        "name": "PointCount",
        "datatype": "integer",
        "type": "aggregation",
        "formula": "=_xlfn.COUNTIFS(scales!SystemID, {{SystemID}})",
        "Description": "Number of scale measurements for this system."
      },
      {
        "name": "MinLogScale",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=_xlfn.MINIFS(scales!LogScale, scales!SystemID, {{SystemID}})",
        "Description": "Minimum log10(Scale) across this system."
      },
      {
        "name": "MaxLogScale",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=_xlfn.MAXIFS(scales!LogScale, scales!SystemID, {{SystemID}})",
        "Description": "Maximum log10(Scale) across this system."
      },
      {
        "name": "MinLogMeasure",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=_xlfn.MINIFS(scales!LogMeasure, scales!SystemID, {{SystemID}})",
        "Description": "Minimum log10(Measure) across this system."
      },
      {
        "name": "MaxLogMeasure",
        "datatype": "number",
        "type": "aggregation",
        "formula": "=_xlfn.MAXIFS(scales!LogMeasure, scales!SystemID, {{SystemID}})",
        "Description": "Maximum log10(Measure) across this system."
      },
      {
        "name": "EmpiricalLogLogSlope",
        "datatype": "number",
        "type": "calculated",
        "formula": "=( {{MinLogMeasure}} - {{MaxLogMeasure}} ) / ( {{MaxLogScale}} - {{MinLogScale}} )",
        "Description": "Approximate slope of log–log line from the extremes, d(log Measure)/d(log Scale)."
      },
      {
        "name": "TheoreticalLogLogSlope",
        "datatype": "number",
        "type": "lookup",
        "formula": "=_xlfn.INDEX(systems!TheoreticalLogLogSlope, _xlfn.MATCH({{SystemID}}, systems!SystemID, 0))",
        "Description": "Copied from systems for comparison."
      }
    ],
